{
  "plugins": ['prettier'],
  "parser": "babel-eslint",
    "env": {
      "es6": true,
      "browser": true,
      "node": true

    },
    "rules": {
    "require-jsdoc": "off",
    "object-curly-spacing": ["error", "always"],
    },
    "extends": [
    "plugin:prettier/recommended",
    "eslint:recommended", "google"] 
}
